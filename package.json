{
  "name": "wr-boilerplate",
  "version": "0.0.1",
  "description": "Template for WR projects",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require jsdom-global/register --require babel-core/register src/**/*.spec.js",
    "build": "webpack",
    "start": "webpack-dev-server --env.devserver --env.conf local",
    "generate-doc": "esdoc -c esdoc.json",
    "//": "Shortcuts for common environments",
    "build-local": "npm run build -- --env.conf local",
    "build-dev": "npm run build -- --env.conf dev",
    "build-pre": "npm run build -- --env.conf pre --env.minify true",
    "build-production": "npm run build -- --env.conf pro --env.minify true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dome-consulting/wr-frontend-boilerplate.git"
  },
  "author": "Dome Consulting & Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dome-consulting/wr-frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/dome-consulting/wr-frontend-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-root-import": "^5.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "colors": "^1.1.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "esdoc": "^0.5.2",
    "esdoc-plugin-dependency-graphs": "^0.1.0",
    "esdoc-plugin-jspm": "^0.6.6",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",    
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.11.0",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
